{
    "url": {
        "base_url": "https://www.eff.org",
        "relative_url": "/who-has-your-back-2014"
    },
    "source_name": "Electronic Frontier Foundation",
    "table_selector":"#starchart > tbody:nth-child(2) > tr",
    "company_info": [
        {
            "label": "Company Name",
            "value": {
                "selector": "td:nth-child(1) > img:nth-child(1)",
                "type": "alt"
            }
        },
        {
            "label": "Logo",
            "value": {
                "selector": "td:nth-child(1) > img",
                "type": "image"
            }
        }
    ],
    "metrics": [
        {
            "label": "Requires warrant for content",
            "value": {
                "selector": "td:nth-child(2) > img",
                "type": "src",
                "replace":{
                    "regex":["/sites/default/files/star_0\\.png","/sites/default/files/checklist-1\\.png"],
                    "with":["Yes","No"]
                }
            }
        },
        {
            "label": "Tells users about government data requests",
            "value": {
                "selector": "td:nth-child(3) > img",
                "type": "src",
                "replace":{
                    "regex":["/sites/default/files/star_0\\.png","/sites/default/files/checklist-1\\.png"],
                    "with":["Yes","No"]
                }
            }
        },
        {
            "label": "Publishes transparency reports",
            "value": {
                "selector": "td:nth-child(4) > img",
                "type": "src",
                "replace":{
                    "regex":["/sites/default/files/star_0\\.png","/sites/default/files/checklist-1\\.png"],
                    "with":["Yes","No"]
                }
            }
        },
        {
            "label": "Publishes law enforcement guidelines",
            "value": {
                "selector": "td:nth-child(5) > img",
                "type": "src",
                "replace":{
                    "regex":["/sites/default/files/star_0\\.png","/sites/default/files/checklist-1\\.png"],
                    "with":["Yes","No"]
                }
            }
        },
        {
            "label": "Fights for users' privacy rights in courts",
            "value": {
                "selector": "td:nth-child(6) > img",
                "type": "src",
                "replace":{
                    "regex":["/sites/default/files/star_0\\.png","/sites/default/files/checklist-1\\.png"],
                    "with":["Yes","No"]
                }
            }
        },
        {
            "label": "Fights for users' privacy rights in Congress",
            "value": {
                "selector": "td:nth-child(7) > img",
                "type": "src",
                "replace":{
                    "regex":["/sites/default/files/star_0\\.png","/sites/default/files/checklist-1\\.png"],
                    "with":["Yes","No"]
                }
            }
        },
        {
            "label": "citeyear",
            "value": "2014"
        }
    ],
    "store": {
        "format": "COMPANY_METRIC",
        "database": "WikiRateDB",
        "companies_collection": "Companies",
        "metrics_collection": "Snippets"
    },
    "dynamic_page": false
}