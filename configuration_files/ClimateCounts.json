            }
        },
        {
            "label": "Logo",
            "value": {
                "selector": "#company_score_company_icon > a > img",
                "type": "src"
            }
        },
        {
            "label": "Website",
            "value": {
                "selector": "#company_score_company_icon > a",
                "type": "link"
            }
        }
    ],
    "metrics": [
        {
            "label": "Review ClimateCounts Score",
            "value": {
                "selector": ".leftcolumn_inner > b:nth-child(3)",
                "type": "text",
                "replace": {
                    "regex": [
                        "Review: ",
                        " points",
                        "\\."
                    ],
