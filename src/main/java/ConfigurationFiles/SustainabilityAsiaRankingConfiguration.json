{
    "url": {
        "baseURL": "http://sustainability-ranking.channelnewsasia.com",
        "relativeURL": "/top100.html"
    },
    "table_selector": "div.col-md-4.col-sm-4",
    "source_name": "Asia's Sustainability Ranking",
    "company_fields": [
        {
            "FieldName": "Company Name",
            "FieldValue": "div:nth-child(1) > h3",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "CSS",
            "FieldNameType": "TEXT",
            "FieldValueType": "TEXT"
        },
        {
            "FieldName": "Country",
            "FieldValue": "div:nth-child(1) > div:nth-child(3) > h5:nth-child(1)",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "CSS",
            "FieldNameType": "TEXT",
            "FieldValueType": "TEXT"
        }
    ],
    "snippet_fields": [
        {
            "FieldName": "Sector",
            "FieldValue": "div:nth-child(1) > div:nth-child(3) > h6:nth-child(2)",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "CSS",
            "FieldNameType": "TEXT",
            "FieldValueType": "TEXT"
        },
        {
            "FieldName": "Asia's Sustainability Ranking",
            "FieldValue": "div:nth-child(1) > div:nth-child(1)",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "CSS",
            "FieldNameType": "TEXT",
            "FieldValueType": "TEXT"
        }
    ],
    "dynamicHTML": false,
    "store": {
        "as": "ABSTRACT_SNIPPET",
        "toHardDrive": "SustainabilityAsiaRanking.json"
    }  
}