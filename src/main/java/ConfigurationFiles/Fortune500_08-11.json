{
    "url": {
        "baseURL": "http://fortune.com",
        "relativeURL": "/fortune500/2008"
    },
    "source_name": "Fortune",
    "table_selector": ".content-well > div:nth-child(1) > article",
    "company_fields": [
        {
            "FieldName": "Company Name",
            "FieldValue": "div.article-title > h1",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "CSS",
            "FieldNameType": "TEXT",
            "FieldValueType": "TEXT"
        },
        {
            "FieldName": "Website",
            "FieldValue": "table.highlights-table > tbody > tr:nth-child(5) > td:nth-child(2)",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "CSS",
            "FieldNameType": "TEXT",
            "FieldValueType": "TEXT"
        },
        {
            "FieldName": "Country",
            "FieldValue": "United States",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "RAW_TEXT",
            "FieldNameType": "TEXT",
            "FieldValueType": "TEXT"
        }
    ],
    "snippet_fields": [
        {
            "FieldName": "Rank in List: Fortune 500",
            "FieldValue": "table.highlights-table > tbody > tr:nth-child(1) > td:nth-child(2)",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "CSS",
            "FieldNameType": "TEXT",
            "FieldValueType": "TEXT"
        },
        {
            "FieldName": "citeyear",
            "FieldValue": "2008",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "RAW_TEXT",
            "FieldNameType": "TEXT",
            "FieldValueType": "TEXT"
        }
    ],
    "dynamicHTML": true,
    "event": {
        "type": "SCROLL_DOWN",
        "timesToRepeat": "500",
        "repetition_type": "AFTER_ALL_EVENTS"
    }
}
