{
  "url": {
    "baseURL": "http://www.rspo.org",
    "relativeURL": "/members/page/"
  },
  "table_selector":".table > tbody:nth-child(2) > tr",
  "source_name":"RSPO",
  "company_fields" : [
    {
      "FieldName": "Company Name",
      "FieldValue": "td:nth-child(1)",
      "SelectorNameType": "RAW_TEXT",
      "SelectorValueType": "CSS",
      "FieldNameType": "TEXT",
      "FieldValueType": "TEXT"
    },
    {
      "FieldName": "Country",
      "FieldValue": "td:nth-child(2)",
      "SelectorNameType": "RAW_TEXT",
      "SelectorValueType": "CSS",
      "FieldNameType": "TEXT",
      "FieldValueType": "TEXT"
    }      
    ],
  "snippet_fields": [
    {
      "FieldName": "RSPO Membership type",
      "FieldValue": "td:nth-child(5)",
      "SelectorNameType": "RAW_TEXT",
      "SelectorValueType": "CSS",
      "FieldNameType": "TEXT",
      "FieldValueType": "TEXT"
    },
    {
      "FieldName": "RSPO Member since",
      "FieldValue": "td:nth-child(4)",
      "SelectorNameType": "RAW_TEXT",
      "SelectorValueType": "CSS",
      "FieldNameType": "TEXT",
      "FieldValueType": "TEXT"
    },
    {
      "FieldName": "RSPO Status",
      "FieldValue": "td:nth-child(3)",
      "SelectorNameType": "RAW_TEXT",
      "SelectorValueType": "CSS",
      "FieldNameType": "TEXT",
      "FieldValueType": "TEXT"
    }
  ],
  "nextPageSelector":".next > a:nth-child(1)",
  "dynamicHTML": false
}