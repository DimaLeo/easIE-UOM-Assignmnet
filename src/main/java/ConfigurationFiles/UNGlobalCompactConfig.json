{
    "url": {
        "baseURL": "https://www.unglobalcompact.org",
        "relativeURL": "/participation/report/cop/create-and-submit/active?page=1&per_page=250"
    },
    "table_selector": ".table-embedded > tbody:nth-child(2) > tr",
    "source_name": "UN Global Compact",
    "company_fields": [
        {
            "FieldName": "Company Name",
            "FieldValue": "td:nth-child(1)",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "CSS",
            "FieldNameType": "TEXT",
            "FieldValueType": "title"
        },
        {
            "FieldName": "Country",
            "FieldValue": "td:nth-child(1)",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "CSS",
            "FieldNameType": "TEXT",
            "FieldValueType": "title"
        },
        {
            "FieldName": "Sector",
            "FieldValue": "td:nth-child(2)",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "CSS",
            "FieldNameType": "TEXT",
            "FieldValueType": "title"
        }
    ],
    "snippet_fields": [
        {
            "FieldName": "Participant Since",
            "FieldValue": "td:nth-child(4)",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "CSS",
            "FieldNameType": "TEXT",
            "FieldValueType": "TEXT"
        },
        {
            "FieldName": "Global Compact Status",
            "FieldValue": "Active",
            "SelectorNameType": "RAW_TEXT",
            "SelectorValueType": "RAW_TEXT",
            "FieldNameType": "TEXT",
            "FieldValueType": "TEXT"
        }
    ],
    "nextPageSelector": ".next_page",
    "dynamicHTML": false,
    "store": {
        "as": "ABSTRACT_SNIPPET",
        "toHardDrive": "UNGlobalCompact.json"
    }
}